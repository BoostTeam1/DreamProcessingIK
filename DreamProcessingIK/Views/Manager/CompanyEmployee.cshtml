@model List<EmployeeListCompanyDto>
@{
    Layout = null;
}
<table class="table table-bordered table-striped">
    <tr>
       
        <th>Adı</th>
        <th>Soyadı</th>
    </tr>
    @if(@Model.Count()==0)
    {
        <tr>
            <td colspan=3>Mevcut çalışanınız yoktur</td>
        </tr>
    }
    else
    {
        @foreach (var item in Model)
       {
           <tr>
              
               <td>@item.FirstName</td>
               <td>@item.LastName</td>
               <td><a class="btn btn-success" asp-controller="Member" asp-action="AddDebit" asp-route-id="@item.UserId"> Zimmet Ekle</a>   </td>
           </tr>
       }
    }
</table>

